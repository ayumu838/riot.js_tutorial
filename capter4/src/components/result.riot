<result>
  <h2>Search Result</h2>
  <Spinner />

  <ul>
    <li each={ gif in gifsJson } ket={ gif.id }>
      <img src={ gif.images.downsized.url } alt={ gif.title } />
    </li>
  </ul>

  <script>
  import Spinner from './spinner'

    export default {
      components: {
        Spinner
      },
      onMounted(props) {
        props.observable.on('show gifs', response => {
          this.gifsJson = response
          this.update()
        })
      }
    }
  </script>
</result>
